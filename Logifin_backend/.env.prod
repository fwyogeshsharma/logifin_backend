# Logifin Backend Production Environment Configuration
# IMPORTANT: Update these values for production deployment

# Database Configuration
DB_NAME=LogifinProd
DB_USER=logifin_user
DB_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
DB_PORT=5432

# Spring Profile
SPRING_PROFILE=prod

# Application Port
APP_PORT=8080

# Logging
SHOW_SQL=false
LOG_LEVEL=WARN

# pgAdmin Configuration (Consider disabling in production)
PGADMIN_EMAIL=admin@yourcompany.com
PGADMIN_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
PGADMIN_PORT=5050
